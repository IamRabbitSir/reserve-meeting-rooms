doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        link(rel="icon", href="/images/login/baobao.ico", type="image/x-icon")
        title 甘其食 - Meeting Room Booking System
        link(rel="stylesheet", type="text/css", href="/stylesheets/css/H-ui.min.css")
        link(rel="stylesheet", type="text/css", href="/stylesheets/css/H-ui.admin.css")
        link(rel="stylesheet", type="text/css", href="/stylesheets/css/iconfont.min.css")
        link#skin(rel="stylesheet", type="text/css", href="/stylesheets/css/skin/skin.css" )
    body
        header.navbar-wrapper
            .navbar.navbar-fixed-top
                .container-fluid.cl
                    img(src="/images/gqsmeet/meet-logo.png", style="height: 40px;")
                    span.logo.navbar-slogan.f-l.mr-10.hidden-xs v1.0
                    nav#Hui-userbar.nav.navbar-nav.navbar-userbar.hidden-xs
                        ul.cl
                            li #{session.user.type}
                            li.dropDown.dropDown_hover
                                a.dropDown_A(href="#")
                                    #{power_name}
                                    | #{session.user.us_name}
                                    i.Hui-iconfont &#xe6d5;
                                ul.dropDown-menu.menu.radius.box-shadow
                                    li
                                        a(href="javascript:;", onclick="myselfinfo()") 个人信息
                                    li
                                        a(href="/users/logout") 注销
                            li#Hui-msg
                                a(href="#", title="消息")
                                    span.badge.badge-danger 1
                                    i.Hui-iconfont(style="font-size:18px") &#xe68a;
                            li#Hui-skin.dropDown.right.dropDown_hover
                                a.dropDown_A(href="javascript:;", title="换肤")
                                    i.Hui-iconfont(style="font-size:18px") &#xe62a;
                                ul.dropDown-menu.menu.radius.box-shadow
                                    li
                                        a(href="javascript:;", data-val="default", title="默认（黑色）") 默认（黑色）
                                    li
                                        a(href="javascript:;", data-val="blue", title="蓝色") 蓝色
                                    li
                                        a(href="javascript:;", data-val="green", title="绿色") 绿色
        aside.Hui-aside
            div.menu_dropdown.bk_2
                - for (var n = 0; n < data.length; n++)
                   dl
                      dt
                         i.Hui-iconfont &#xe616;&nbsp;
                            = data[n].menu_name
                         i.Hui-iconfont.menu_dropdown-arrow &#xe6d5;
                      dd
                         ul
                            - for (var i = 0; i < data2.length; i++)
                               if data[n].menu_id == data2[i].menu_parent_id
                                   li
                                      a(data-href=data2[i].menu_path, data-title=data2[i].menu_name, href="javascript:void(0)")
                                         = data2[i].menu_name
        .dislpayArrow.hidden-xs
            a.pngfix(href="javascript:void(0);", onclick="displaynavbar(this)")
        section.Hui-article-box
            #Hui-tabNav.Hui-tabNav.hidden-xs
                .Hui-tabNav-wp
                    ul#min_title_list.acrossTab.cl
                        li.active
                            span(title="我的首页", data-href="/meet/gqsMeet/default") 首页
                            em
                .Hui-tabNav-more.btn-group
                    a#js-tabNav-prev.btn.radius.btn-default.size-S(href="javascript:;")
                        i.Hui-iconfont &#xe6d4;
                    a#js-tabNav-next.btn.radius.btn-default.size-S(href="javascript:;")
                        i.Hui-iconfont &#xe6d7;
            #iframe_box.Hui-article
                .show_iframe
                    .loading(style="display:none")
                    iframe(scrolling="yes", frameborder="0", src="/meet/gqsMeet/default")
        #Huiadminmenu.contextMenu
            ul
                li#closethis 关闭当前
                li#closeall 关闭全部
        // _footer 作为公共模版分离出去
        include _footer
        // 请在下方写此页面业务相关的脚本
        script(type="text/javascript", src="/javascripts/gqsmeet/lib/jquery.contextmenu.r2.js")
        script(type="text/javascript").
            /*个人信息*/
            function myselfinfo() {
                layer.open({
                    type: 1,
                    area: ['300px', '200px'],
                    fix: false, //不固定
                    maxmin: true,
                    shade: 0.4,
                    title: '查看信息',
                    content: '<div>管理员信息</div>'
                });
            }


